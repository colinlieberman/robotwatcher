<p id="notice"><%= notice %></p>

<h1>Worker Readings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @worker_readings.each do |worker_reading| %>
      <tr>
        <td><%= link_to 'Show', worker_reading %></td>
        <td><%= link_to 'Edit', edit_worker_reading_path(worker_reading) %></td>
        <td><%= link_to 'Destroy', worker_reading, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Worker Reading', new_worker_reading_path %>
